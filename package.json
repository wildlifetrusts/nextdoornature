{
  "name": "nextdoornature",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run generate && vite build",
    "generate": "npm run generate_guides && npm run generate_stories && npm run generate_pages",
    "generate_pages": "m2j -o ./data/pages.json -c ./content/pages/en/*.md",
    "generate_guides": "m2j -o ./data/guides.json -c ./content/guides/en/*.md",
    "generate_stories": "m2j -o ./data/stories.json -c ./content/stories/en/*.md",
    "format": "elm-format src",
    "lint": "npx elm-review",
    "test": "elm-test",
    "prepare": "husky install",
    "postinstall": "elm-tooling install && npm run generate"
  },
  "devDependencies": {
    "elm": "0.19.1-5",
    "elm-review": "^2.9.1",
    "elm-test": "^0.19.1-revision12",
    "elm-tooling": "1.13.0",
    "markdown-to-json": "^0.5.4",
    "husky": "^8.0.3",
    "prettier": "2.8.4",
    "vite": "4.1.4",
    "vite-plugin-elm": "2.8.0-beta.3"
  },
  "dependencies": {}
}
